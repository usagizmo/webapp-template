# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{txt,tsv,csv}]
indent_style = tab
insert_final_newline = false
trim_trailing_whitespace = false
