    "snakecase-keys": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.525.0",
    "@markuplint/svelte-parser": "^4.7.9",
    "@markuplint/svelte-spec": "^4.5.19",
    "@repo/eslint-config": "workspace:*",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.22.2",
