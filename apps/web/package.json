    "mode-watcher": "^1.1.0",
    "npm-run-all2": "^8.0.4",
    "svelte": "^5.35.4",
    "svelte-check": "^4.2.2",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "^7.0.3",
    "vitest": "^3.2.4"
