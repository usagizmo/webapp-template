{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:path": "pathpida --ignorePath .gitignore --enableStatic",
    "generate": "run-p generate:*",
    "build:next": "next build",
    "build": "run-s generate build:next",
    "dev:path": "pathpida --ignorePath .gitignore --enableStatic --watch",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next && rm -rf src/lib/\\$path.ts"
  },
  "dependencies": {
    "firebase": "9.6.9",
    "firebase-admin": "10.0.2",
    "next": "12.1.0",
    "next-auth": "4.3.1",
    "phosphor-react": "1.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.28.1",
    "react-query": "3.34.16",
    "ulid": "2.3.0",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@firebase/app-compat": "0.1.20",
    "@firebase/app-types": "0.7.0",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/node": "17.0.21",
    "@types/react": "17.0.41",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.4",
    "constants": "workspace:*",
    "eslint-preset": "workspace:*",
    "generated": "workspace:*",
    "graphql": "16.3.0",
    "lintstagedrc": "workspace:*",
    "next-transpile-modules": "9.0.0",
    "pathpida": "0.18.0",
    "postcss": "8.4.12",
    "tailwind-config-base": "workspace:*",
    "tailwindcss": "3.0.23",
    "tsconfig": "workspace:*",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tslib": "2.3.1",
    "types": "workspace:*",
    "typescript": "4.6.2",
    "ui": "workspace:*",
    "utils": "workspace:*",
    "webpack": "5.70.0"
