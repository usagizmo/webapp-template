{
  "name": "web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate:svelte": "svelte-kit sync",
    "generate:pull-schema": "node --require=dotenv/config ./commands/pull-schema.js",
    "generate": "concurrently pnpm:generate:*",
    "dev": "vite dev --port 3000 --open",
    "build": "pnpm generate && vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:markup": "markuplint \"src/**/*.svelte\"",
    "lint:js": "TIMING=1 eslint \"src\"",
    "lint:cspell": "cspell \"src/**\"",
    "lint:prettier": "prettier --check --ignore-path=../../.prettierignore .",
    "lint": "concurrently pnpm:lint:*",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@nhost/nhost-js": "^1.7.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "luxon": "^3.1.1",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@types/luxon": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint-config-custom": "workspace:*",
    "graphql": "^16.0.0",
    "graphqurl": "^1.0.1",
    "houdini": "^0.18.1",
    "houdini-svelte": "^0.18.1",
    "postcss": "^8.4.20",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tailwind-preset-base": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "ui": "workspace:*",
    "vite": "^4.0.1"
  }
}
