{
  "name": "web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000 --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:markup": "markuplint \"src/**/*.svelte\"",
    "lint:js": "TIMING=1 eslint \"src\"",
    "lint:cspell": "cspell \"src/**\"",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && concurrently pnpm:lint:*",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.90",
    "@sveltejs/kit": "1.0.0-next.570",
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.19",
    "svelte": "^3.53.1",
    "svelte-check": "^2.10.0",
    "svelte-preprocess": "^4.10.7",
    "tailwind-preset-base": "workspace:*",
    "tailwindcss": "3.1.8",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "ui": "workspace:*",
    "vite": "^3.2.4"
  }
}
