{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:path": "pathpida --ignorePath .gitignore --enableStatic --output ./src/generated",
    "generate:graphql": "cross-env DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config graphql/codegen.yml",
    "generate": "run-p generate:*",
    "build:next": "next build",
    "build": "run-s generate build:next",
    "dev:path": "pathpida --ignorePath .gitignore --enableStatic --output ./src/generated --watch",
    "dev:graphql": "cross-env DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config graphql/codegen.yml --watch",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "firebase": "9.6.7",
    "firebase-admin": "10.0.2",
    "next": "12.0.8",
    "next-auth": "4.2.1",
    "phosphor-react": "1.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.1",
    "react-query": "3.34.16",
    "ui": "workspace:*",
    "ulid": "2.3.0",
    "zustand": "3.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@firebase/app-compat": "0.1.18",
    "@firebase/app-types": "0.7.0",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-query": "3.5.5",
    "@types/node": "17.0.12",
    "@types/react": "17.0.37",
    "autoprefixer": "10.4.2",
    "config": "workspace:*",
    "graphql": "16.3.0",
    "lintstagedrc": "workspace:*",
    "next-transpile-modules": "9.0.0",
    "pathpida": "0.18.0",
    "postcss": "8.4.7",
    "tailwindcss": "3.0.23",
    "tsconfig": "workspace:*",
    "tslib": "2.3.1",
    "typescript": "4.5.3"
  },
  "peerDependencies": {
    "graphql": "16.3.0"
  }
}
