{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:path": "pathpida --ignorePath .gitignore --enableStatic --output ./src/generated",
    "generate:graphql": "cross-env DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config graphql/codegen.yml",
    "generate": "run-p generate:*",
    "build:next": "next build",
    "build": "run-s generate build:next",
    "dev:path": "pathpida --ignorePath .gitignore --enableStatic --output ./src/generated --watch",
    "dev:graphql": "cross-env DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config graphql/codegen.yml --watch",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "firebase": "9.6.8",
    "firebase-admin": "10.0.2",
    "next": "12.1.0",
    "next-auth": "4.2.1",
    "phosphor-react": "1.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.1",
    "react-query": "3.34.16",
    "ulid": "2.3.0",
    "zustand": "3.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@firebase/app-compat": "0.1.19",
    "@firebase/app-types": "0.7.0",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.7",
    "@graphql-codegen/typescript-operations": "2.3.4",
    "@graphql-codegen/typescript-react-query": "3.5.7",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "eslint-preset": "workspace:*",
    "graphql": "16.3.0",
    "lintstagedrc": "workspace:*",
    "pathpida": "0.18.0",
    "postcss": "8.4.8",
    "tailwind-config-base": "workspace:*",
    "tailwindcss": "3.0.23",
    "tsconfig": "workspace:*",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tslib": "2.3.1",
    "typescript": "4.6.2",
    "webpack": "5.70.0"
  },
  "peerDependencies": {
    "graphql": "16.3.0",
    "webpack": "5.70.0"
  }
}
