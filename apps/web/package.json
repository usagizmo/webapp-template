{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "echo 'Types are directly imported from backend'",
    "dev": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:markup": "markuplint \"**\"",
    "lint:js": "eslint .",
    "lint:cspell": "cspell \"**\"",
    "lint:prettier": "prettier . --check --ignore-path=../../.prettierignore",
    "lint": "run-p lint:*",
    "test:watch": "vitest",
    "test": "vitest run",
    "format:js": "eslint --fix .",
    "format:prettier": "prettier . --write --ignore-path=../../.prettierignore",
    "format": "run-p format:*"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "camelcase-keys": "^9.1.3",
    "cdate": "^0.0.7",
    "snakecase-keys": "^8.0.1",
    "tailwind-variants": "^1.0.0"
  },
  "devDependencies": {
    "@markuplint/svelte-parser": "^4.7.9",
    "@markuplint/svelte-spec": "^4.5.19",
    "@repo/eslint-config": "workspace:*",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "npm-run-all2": "^8.0.4",
    "svelte": "^5.34.1",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^4.1.10",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
