  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:markup": "markuplint \"**\"",
