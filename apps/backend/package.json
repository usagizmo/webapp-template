{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pull": "supabase db pull --local --schema public,auth,storage",
    "generate:supabase-types": "mkdir -p \\$generated && supabase gen types typescript --local > ./\\$generated/supabase-types.ts",
    "generate:shared-types": "cd ../../packages/types && pnpm generate",
    "generate": "concurrently pnpm:generate:*",
    "start": "supabase start",
    "stop": "supabase stop"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "supabase": "^2.24.3"
  }
}
