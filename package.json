{
  "name": "nextjs-template",
  "version": "0.13.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev:path": "pathpida --ignorePath .gitignore --output ./src/generated --watch",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "build:path": "pathpida --ignorePath .gitignore --output ./src/generated",
    "build:graphql": "pnpm generate",
    "build:next": "next build",
    "build": "npm-run-all -p build:path build:graphql -s build:next",
    "start": "next start",
    "lint": "next lint --dir src",
    "format": "prettier --write .",
    "hasura:migrate:apply": "hasura --project hasura --envfile ../.env.local migrate apply",
    "hasura:migrate:export": "hasura --project hasura --envfile ../.env.local migrate create init --from-server",
    "hasura:seed:apply": "hasura --project hasura --envfile ../.env.local seed apply",
    "hasura:seed:export": "hasura --project hasura --envfile ../.env.local seed create tables_seed",
    "hasura:metadata:apply": "hasura --project hasura --envfile ../.env.local metadata apply",
    "hasura:metadata:export": "hasura --project hasura --envfile ../.env.local metadata export",
    "generate": "cross-env DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config graphql/codegen.yml"
  },
  "dependencies": {
    "firebase": "9.6.7",
    "firebase-admin": "10.0.2",
    "graphql": "16.3.0",
    "graphql-request": "4.0.0",
    "next": "12.1.0",
    "next-auth": "4.2.1",
    "phosphor-react": "1.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "react-query": "3.34.15",
    "ulid": "2.3.0",
    "zustand": "3.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@firebase/app-compat": "0.1.18",
    "@firebase/app-types": "0.7.0",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-query": "3.5.5",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "autoprefixer": "10.4.2",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.3.0",
    "hasura-cli": "2.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "0.1.7",
    "tailwindcss": "3.0.23",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache",
      "prettier --write"
    ],
    "*": "prettier --write --ignore-unknown"
  },
  "peerDependencies": {
    "@babel/core": "7.16.12",
    "@firebase/app-compat": "0.1.17",
    "@firebase/app-types": "0.7.0",
    "@typescript-eslint/parser": "5.10.1",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5"
  }
}
