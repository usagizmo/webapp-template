{
  "parser": {
    "\\.svelte$": "@markuplint/svelte-parser"
  },
  "extends": [
    "markuplint:recommended"
  ],
  "excludeFiles": [
    "./apps/web/src/app.html"
  ],
  "rules": {
    "character-reference": false,
    "ineffective-attr": false,
    "label-has-control": false,
    "require-accessible-name": false
  }
}
