{
  "name": "@repo/eslint-config",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./eslint.config.js"
  },
  "scripts": {
    "lint:cspell": "cspell \"**\"",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check --ignore-path=../../.prettierignore .",
    "lint": "concurrently pnpm:lint:*",
