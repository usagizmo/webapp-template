    "format": "concurrently pnpm:format:*"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "globals": "^15.0.0",
    "svelte-eslint-parser": "^0.34.1",
    "typescript-eslint": "^7.7.0"
  }
}
