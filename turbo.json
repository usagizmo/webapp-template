{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "web#generate": {
      "outputs": [".svelte-kit/**", "src/lib/$generated/**"]
    },
    "build": {},
    "mockup#build": {
      "outputs": ["public/styles.css"]
    },
    "web#build": {
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "web#lint": {
      "outputs": []
    },
    "test": {},
    "mockup#test": {
      "dependsOn": ["mockup#build"],
      "outputs": ["tests/external-links.txt"]
    },
    "format": {}
  }
}
