{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["GRAPHQL_ENDPOINT", "HASURA_ADMIN_SECRET", "HASURA_GRAPHQL_ADMIN_SECRET"],
  "pipeline": {
    "web#generate": {
      "outputs": [".svelte-kit/**"]
    },
    "mockup#build": {
      "outputs": ["public/styles.css"]
    },
    "story#build": {
      "outputs": ["storybook-static/**"]
    },
    "web#build": {
      "dependsOn": ["web#generate"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "web#lint": {
      "dependsOn": ["web#generate"],
      "outputs": []
    },
    "test": {},
    "mockup#test": {
      "dependsOn": ["mockup#build"],
      "outputs": ["tests/external-links.txt"]
    },
    "format": {}
  }
}
