{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "mockup#build": {
      "dependsOn": ["^build"],
      "outputs": ["public/script.js", "public/styles.css"]
    },
    "storybook#build": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "src/lib/$path.ts"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {},
    "mockup#test": {
      "dependsOn": ["mockup#build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
